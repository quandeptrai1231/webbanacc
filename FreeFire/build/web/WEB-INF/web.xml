<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>napthe</servlet-name>
        <servlet-class>napthe.napthe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hotro</servlet-name>
        <servlet-class>hotro.hotro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>momo</servlet-name>
        <servlet-class>nganhang.momo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>atm</servlet-name>
        <servlet-class>nganhang.atm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>napthe</servlet-name>
        <url-pattern>/napthe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hotro</servlet-name>
        <url-pattern>/hotro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>momo</servlet-name>
        <url-pattern>/momo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>atm</servlet-name>
        <url-pattern>/atm</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
